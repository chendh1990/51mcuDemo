 200                  void OSStart(void)
 201                  
 202          {
 203   1          uint8 idata *cp;
 204   1          uint8 i;
 205   1          
 206   1          cp = STACK;
 207   1          
 208   1          OSTsakStackBotton[0] = STACK;
 209   1          OSTsakStackBotton[OS_MAX_TASKS + 1] = (uint8 idata *)(IDATA_RAM_SIZE % 256);
